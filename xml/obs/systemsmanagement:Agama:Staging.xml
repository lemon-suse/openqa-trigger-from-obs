<openQA
    project_pattern="systemsmanagement:Agama:Staging"
    dist_path="obspublish-other::openqa/systemsmanagement:Agama:Staging/"
    distri="alp"
    version="agama-3.0-staging"
    archs="x86_64 aarch64 ppc64le s390x">
    <flavor name="agama-live" folder="images/*/agama-live">
        <hdd filemask="agama-live.*-Build.*iso$"/>
    </flavor>
    <flavor name="agama-live-ALP" folder="images/*/agama-live:ALP">
        <hdd filemask="agama-live.*-ALP-Build.*iso$"/>
    </flavor>
    <flavor name="agama-live-ALP-Playwright" folder="images/*/agama-live:ALP-Playwright">
        <hdd filemask="agama-live.*-ALP-Playwright-Build.*iso$"/>
    </flavor>
    <flavor name="agama-live-openSUSE" folder="images/*/agama-live:openSUSE">
        <hdd filemask="agama-live.*-openSUSE-Build.*iso$"/>
    </flavor>
    <flavor name="agama-live-openSUSE-Playwright" folder="images/*/agama-live:openSUSE-Playwright">
        <hdd filemask="agama-live.*-openSUSE-Playwright-Build.*iso$"/>
    </flavor>
</openQA>
